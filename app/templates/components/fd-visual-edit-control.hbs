<div class="field">
    <label>{{prototypeByDropdownCaption}}</label>
    {{flexberry-dropdown
      items=popertyBy
      value=model.prototypeBy
      class=undefined
    }}
</div>
<div class="field">
    <label>{{nameTextboxCaption}}</label>
    {{flexberry-textbox
      value=model.name
    }}
</div>
<div class="field">
    <label>{{typeDropdownCaption}}</label>
    {{flexberry-dropdown
      items=componentTypes
      value=model.type
      class=undefined
    }}
</div>
<div class="field">
    <label>{{isNullCheckboxCaption}}</label>
    {{flexberry-checkbox
      class="fluid"
      value=model.isNull
    }}
</div>
<div class="field">
    <label>{{defaultValueTextboxCaption}}</label>
    {{flexberry-textbox
      value=model.defaultValue
    }}
</div>
