<h3 class="ui header">
  {{t 'forms.fd-aggregation-edit-form.caption'}}
  <div class="sub header">
    {{startClassName}} - {{endClassName}}
  </div>
</h3>

{{#if errorMessages}}
  {{#each errorMessages as |currentError|}}
    {{ui-message
      type='error'
      closeable=true
      visible=true
      title='Error occurred'
      message=currentError
    }}
  {{/each}}
{{/if}}
<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=model.errors}}
    </div>
  </div>
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button type="submit" class="ui button save-button" {{action 'save'}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        <button type="submit" class="ui button save-close-button" {{action 'saveAndClose'}}>
          {{t "forms.edit-form.saveAndClose-button-text"}}
        </button>
      {{/unless}}
      <button type="submit" class="ui button close-button" {{action 'close'}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.name-caption"}}</label>
    {{
      flexberry-textbox
      value=model.name
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.start-class-caption"}}</label>
    {{
      flexberry-dropdown
      items=implementationsName
      value=startClassName
      readonly=readonlyClass
      onChange=(action "changeStartClass")
    }}
    {{flexberry-validationmessage error=model.errors.startClass pointing="pointing"}}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.end-class-caption"}}</label>
    {{
      flexberry-dropdown
      items=implementationsName
      value=endClassName
      readonly=readonlyClass
      onChange=(action "changeEndClass")
    }}
    {{flexberry-validationmessage error=model.errors.endClass pointing="pointing"}}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.description-caption"}}</label>
    {{
      flexberry-textarea
      value=model.description
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.start-role-caption"}}</label>
    {{
      flexberry-textbox
      value=model.startRole
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.end-role-caption"}}</label>
    {{
      flexberry-textbox
      value=model.endRole
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.start-multiplicity-caption"}}</label>
    {{
      flexberry-textbox
      value=model.startMultiplicity
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.end-role-stored-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.endRoleStored
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.aggregation-auto-generate-type-usage-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.autoGenerateTypeUsage
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.aggregation-type-usage-caption"}}</label>
    {{
      flexberry-textbox
      value=model.typeUsage
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.storage"}}</label>
    {{
      flexberry-textbox
      value=model.storage
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.auto-store-master-disabled-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.autoStoreMasterDisabled
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.detail-auto-generate-type-usage-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.autoGenerateDetailTypeUsage
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.detail-type-usage-caption"}}</label>
    {{
      flexberry-textbox
      value=model.detailTypeUsage
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.pb-aggregation-custom-attributes-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.pBAggregatorCustomAttributes
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.pb-aggregation-get-start-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.pBAggregatorGetStart
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.pb-aggregation-get-end-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.pBAggregatorGetEnd
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.pb-aggregation-set-start-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.pBAggregatorSetStart
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.pb-aggregation-set-end-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.pBAggregatorSetEnd
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.pb-detail-custom-attributes-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.pBDetailCustomAttributes
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.pb-detail-get-start-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.pBDetailGetStart
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.pb-detail-get-end-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.pBDetailGetEnd
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.pb-detail-set-start-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.pBDetailSetStart
    }}
  </div>
  <div class="field">
    <label>{{t "forms.fd-aggregation-edit-form.pb-detail-set-end-caption"}}</label>
    {{
      flexberry-checkbox
      value=model.pBDetailSetEnd
    }}
  </div>
</form>