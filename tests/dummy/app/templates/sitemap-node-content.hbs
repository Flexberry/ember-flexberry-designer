<div class="title-item-menu" onclick={{action 'subMenuEl' bubbles=false}}>
  <i class="icon {{model.icon}}"></i>
  {{model.caption}}
</div>
{{#if model.children.length}}
  <div class="menu subMenu hidden" >
    {{#each model.children as |child|}}
      {{render "sitemap-node" child}}
    {{/each}}
  </div>
{{/if}}