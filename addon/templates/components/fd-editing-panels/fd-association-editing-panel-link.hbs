<div class="ui stackable grid fd-list">

  <div class="row">
    <div class="five wide column">
      {{t "components.fd-association-editing-panel-link.description-caption"}}
    </div>
    <div class="eleven wide column light-text">
      {{flexberry-textarea
        value=model.description
        class='fluid input'
        placeholder=(t "components.fd-association-editing-panel-link.description-placeholder")
        readonly=readonly
      }}
    </div>
  </div>

  <div class="row">
    <div class="five wide column">
      {{t "components.fd-association-editing-panel-link.start-role-caption"}}
    </div>
    <div class="eleven wide column light-text">
      {{flexberry-textbox
        value=model.startRole
        class='fluid input'
        placeholder=(t "components.fd-association-editing-panel-link.start-role-placeholder")
        readonly=readonly
      }}
    </div>
  </div>

  <div class="row">
    <div class="five wide column">
      {{t "components.fd-association-editing-panel-link.start-role-publish-name-caption"}}
    </div>
    <div class="eleven wide column light-text">
      {{flexberry-textbox
        value=model.startRolePublishName
        class='fluid input'
        placeholder=(t "components.fd-association-editing-panel-link.start-role-publish-name-placeholder")
        readonly=readonly
      }}
    </div>
  </div>

  <div class="row">
    <div class="five wide column">
      {{t "components.fd-association-editing-panel-link.start-multiplicity-caption"}}
    </div>
    <div class="eleven wide column light-text">
      {{flexberry-textbox
        value=model.startMultiplicity
        class='fluid input'
        placeholder=(t "components.fd-association-editing-panel-link.start-multiplicity-placeholder")
        readonly=readonly
      }}
    </div>
  </div>

  <div class="row">
    <div class="five wide column">
      {{t "components.fd-association-editing-panel-link.end-role-access-modifier-caption"}}
    </div>
    <div class="eleven wide column light-text">
      {{flexberry-dropdown
        items=(flexberry-enum 's-t-o-r-m-c-a-s-e-repository-access-modifier')
        value=model.startRoleAccessModifier
        class='compact input'
        readonly=readonly
      }}
    </div>
  </div>

  <div class="row">
    <div class="five wide column">
      {{t "components.fd-association-editing-panel-link.start-role-stored-caption"}}
    </div>
    <div class="eleven wide column light-text">
      {{flexberry-checkbox
        value=model.startRoleStored
        class='fluid input'
        readonly=readonly
      }}
    </div>
  </div>

  <div class="row">
    <div class="five wide column">
      {{t "components.fd-association-editing-panel-link.end-role-caption"}}
    </div>
    <div class="eleven wide column light-text">
      {{flexberry-textbox
        value=model.endRole
        class='fluid input'
        placeholder=(t "components.fd-association-editing-panel-link.end-role-placeholder")
        readonly=readonly
      }}
    </div>
  </div>

  <div class="row">
    <div class="five wide column">
      {{t "components.fd-association-editing-panel-link.end-role-publish-name-caption"}}
    </div>
    <div class="eleven wide column light-text">
      {{flexberry-textbox
        value=model.endRolePublishName
        class='fluid input'
        placeholder=(t "components.fd-association-editing-panel-link.end-role-publish-name-placeholder")
        readonly=readonly
      }}
    </div>
  </div>

  <div class="row">
    <div class="five wide column">
      {{t "components.fd-association-editing-panel-link.end-multiplicity-caption"}}
    </div>
    <div class="eleven wide column light-text">
      {{flexberry-textbox
        value=model.endMultiplicity
        class='fluid input'
        placeholder=(t "components.fd-association-editing-panel-link.end-multiplicity-placeholder")
