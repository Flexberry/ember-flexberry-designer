    <div class="row">
      {{
        fd-button
        tagName="label"
        caption="Создать интерфейс через Т"
        click=(action "createClass" "«interface»")
      }}
    </div>
    <div class="row">
      {{
        fd-button
        tagName="label"
        caption="Создать пользовательская форма через Т"
        click=(action "createClass" "«userform»")
      }}
    </div>
    <div class="row">
      {{
        fd-button
        tagName="label"
        caption="Создать перечисление через Т"
        click=(action "createClass" "«enumeration»")
      }}
    </div>
    <div class="row">
      {{
        fd-button
        tagName="label"
        caption="Создать typedef через Т"
        click=(action "createClass" "«typedef»")
      }}
    </div>
  </div>
</form>